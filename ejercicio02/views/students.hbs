<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{title}}</title>
</head>
<body>
    <h1>{{title}}</h1>
    <table border="1" cellpadding="5" cellspacing="0">
        <thead>
            <tr>
                <th>Nombre</th>
                <th>Nota</th>
            </tr>
        </thead>
        <tbody>
            {{#each students}}
            <tr>
            <td>{{this.nombre}}</td>
            <td>
                {{#if this.resaltado}}
                <strong style="color: green;">{{this.nota}}</strong>
                {{else}}
                {{this.nota}}
                {{/if}}
            </td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</body>
</html>

